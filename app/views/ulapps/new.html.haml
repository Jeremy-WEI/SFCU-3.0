            .col-md-9
              = f.text_field :employee2_address_line2, class: "form-control", placeholder: "Address Line2"
          .row
            .col-md-1
              City
            .col-md-3
              = f.text_field :employee2_address_city, placeholder: "City", class: "form-control"
            .col-md-1
              State
            .col-md-3
              = f.text_field :employee2_address_state, placeholder: "State", class: "form-control"
            .col-md-1
              ZIP
            .col-md-3
              = f.text_field :employee2_address_zip, placeholder: "ZIP", class: "form-control"
          .row
            .col-md-1
              Country
            .col-md-4
              = f.select :employee2_country, Country.all.sort, {}, {class: "form-control"}
          %br
          %b Position
          .row
            .col-md-4
              = f.text_field :position2, class: "form-control"
          %br
          %b Supervisor Name
          .row
            .col-md-4
              = f.text_field :supervisor2_firstname, placeholder: "First Name", class: "form-control"
            .col-md-4
