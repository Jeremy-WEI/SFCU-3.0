            .col-md-3
              %label.radio-inline
                = f.radio_button :disbursement, "Month"
                Month
            .col-md-3
              %label.radio-inline
                = f.radio_button :disbursement, "Year"
                Year
          %br
          %b Date Started/Starting
          .row
            .col-md-4
              = f.date_select :startingdate
          %br
          %b Will this job continue for term of this loan?
          .row
            .col-md-3
              %label.radio-inline
                = f.radio_button :continue, false
                No
            .col-md-3
              %label.radio-inline
                = f.radio_button :continue, true
                Yes
          %br
          %b If "No", please list expected duration
          .row
            .col-md-12
              = f.text_area :no, class: "form-control"
          %br
          %b Additional Information
          .row
            .col-md-12
              = f.text_area :additional, class: "form-control"
          %br
        .well
          %h3.text-center Employment II
          %b Employer
          .row
            .col-md-4
              = f.text_field :employer2, class: "form-control"
          %br
          %b Employer Address
          .row
            .col-md-3
              Address Line 1
            .col-md-9
              = f.text_field :employee2_address_line1, class: "form-control", placeholder: "Address Line1"
          .row
            .col-md-3
