              .col-md-4
                %b Name of Depository
              .col-md-4
                %b Type of Account
              .col-md-4
                %b Current Balance
            .row
              .col-md-4
                = f.text_field :depository1, {class: "form-control"}
              .col-md-4
                = f.select :account_type1, ([['Checking', 1], ['Money Market', 2], ['Savings', 3], ['Other',4]]), {}, {class: "form-control"}
              .col-md-4
                = f.text_field :balance1, {class: "form-control"}
            %br
          #bank_account_2
            .well
              %h4.text-center Bank Account2
              %br
