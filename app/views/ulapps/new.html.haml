  $(document).ready(function() {
    	$('#rootwizard').bootstrapWizard({onTabShow: function(tab, navigation, index) {
  		var $total = navigation.find('li').length;
  		var $current = index+1;
  		var $percent = ($current/$total) * 100;
  		$('#rootwizard').find('.bar').css({width:$percent+'%'});
  		for ( var i = 2; i <= 6; i++ ) {
            $("#file_option_"+i).hide();
        }
        var $count_file = 2;
        $('#add_another_file').click(function(){
          $("#file_option_"+$count_file).show();
          $count_file++;
          if ($count_file>6){
            $('#add_another_file').hide();
          }
        });
        for ( var i = 2; i <= 3; i++ ) {
            $("#source_"+i).hide();
        }
        var $count_source = 2;
        $('#add_another_source').click(function(){
          $("#source_"+$count_source).show();
          $count_source++;
          if ($count_source>3){
            $('#add_another_source').hide();
          }
        });
        for ( var i = 2; i <= 3; i++ ) {
            $("#bank_account_"+i).hide();
        }
        var $count_account = 2;
        $('#add_another_bankaccount').click(function(){
          $("#bank_account_"+$count_account).show();
          $count_account++;
          if ($count_account>3){
            $('#add_another_bankaccount').hide();
          }
        });
        for ( var i = 2; i <= 3; i++ ) {
            $("#property_"+i).hide();
        }
        var $count_property = 2;
        $('#add_another_property').click(function(){
          $("#property_"+$count_property).show();
          $count_property++;
          if ($count_property>3){
            $('#add_another_property').hide();
          }
        });
        for ( var i = 2; i <= 4; i++ ) {
            $("#creditor_"+i).hide();
        }
        var $count_creditor = 2;
        $('#add_another_creditor').click(function(){
          $("#creditor_"+$count_creditor).show();
          $count_creditor++;
          if ($count_creditor>4){
            $('#add_another_creditor').hide();
          }
        });

        $('#ulapp_same').change(function(){
          if(this.checked){
            $("#permanent_address").hide();
          }else{
            $("#permanent_address").show();
          }

        });


  	}});
  });






