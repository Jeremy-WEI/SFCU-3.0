              %br
              .row
                .col-md-4
                  %b Name of Depository
                .col-md-4
                  %b Type of Account
                .col-md-4
                  %b Current Balance
              %br
              .row
                .col-md-4
                  = f.text_field :depository3, {class: "form-control"}
                .col-md-4
                  = f.select :account_type3, ([['Checking', 1], ['Money Market', 2], ['Savings', 3], ['Other',4]]), {}, {class: "form-control"}
                .col-md-4
                  = f.text_field :balance3, {class: "form-control"}
              %br
          .row
            .col-md-4
              %button{id: "add_another_bankaccount", type: "button"} Add Another Bank Account
        .well
