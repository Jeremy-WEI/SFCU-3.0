            .col-md-4
              = f.select :employee_country, Country.all.sort, {}, {class: "form-control"}
          %br
          %b Position
          .row
            .col-md-4
              = f.text_field :position, class: "form-control"
          %br
          %b Supervisor Name*
          .row
            .col-md-4
              = f.text_field :supervisor_firstname, placeholder: "First Name", class: "form-control"
            .col-md-4
              = f.text_field :supervisor_lastname, placeholder: "Last Name", class: "form-control"
          %br
          %b GrossPay*
          .row
